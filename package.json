{
  "name": "slavkoscore",
  "version": "3.0.0",
  "description": "AI-powered code analysis platform with predictive kernel monitoring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "slavkoscore": "./dist/cli.js"
  },
  "files": [
    "dist",
    "rules",
    "templates"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts src/server.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts src/server.ts --format cjs,esm --dts --watch",
    "dev:dashboard": "concurrently \"npm run dev\" \"npm run dashboard:dev\"",
    "dashboard:dev": "cd src/dashboard/client && vite",
    "dashboard:build": "cd src/dashboard/client && vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "analyze:self": "node dist/cli.js analyze src --ai",
    "benchmark": "node dist/benchmark.js",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "code-analysis",
    "security",
    "performance",
    "ai",
    "static-analysis",
    "kernel-monitoring",
    "predictive-analysis"
  ],
  "author": "Gertner",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gertner/slavkoscore"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "cli-table3": "^0.6.5",
    "dotenv": "^16.4.5",
    "glob": "^10.4.1",
    "chokidar": "^3.6.0",
    "express": "^4.19.2",
    "ws": "^8.17.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.21.2",
    "tree-sitter-typescript": "^0.21.2",
    "acorn": "^8.11.3",
    "esprima": "^4.0.1",
    "@babel/parser": "^7.24.6",
    "@babel/traverse": "^7.24.6",
    
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    
    "prettier": "^3.3.1",
    
    "openai": "^4.47.3",
    "@anthropic-ai/sdk": "^0.20.9",
    "ollama": "^0.5.1",
    
    "react": "^18.3.1",
    "vue": "^3.4.27",
    "svelte": "^4.2.17",
    
    "webpack": "^5.91.0",
    "vite": "^5.2.12",
    "esbuild": "^0.21.4",
    "@swc/core": "^1.5.7",
    
    "nx": "^19.1.2",
    "lerna": "^8.1.3",
    "turborepo": "^2.0.3",
    
    "lighthouse": "^12.1.0",
    "puppeteer": "^22.10.0",
    "playwright": "^1.44.1",
    
